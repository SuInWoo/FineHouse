\n        jeon: parseInt(this.house.보증금),\n        floor: String(this.house.층),\n      };\n\n      if (this.house.월세 == 0) {\n        likeHouse.type = \"전세\";\n      }\n\n      console.log(\"찜목록에 추가된 하우스\", likeHouse);\n      console.log(\"stringfy\", JSON.stringify(likeHouse));\n      http\n        .post(`/like/write`, JSON.stringify(likeHouse))\n        .then((response) => {\n          console.log(response);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    deleteLikeHouse() {},\n  },\n};\n</script>\n\n<style scoped>\n.mb-3 {\n  margin-top: 20px;\n}\n</style>\n"],"mappings":"AA4DA,SAAAA,QAAA;AACA,OAAAC,iBAAA;AACA,SAAAC,WAAA;AAEA,MAAAC,IAAA,GAAAD,WAAA;AACA,MAAAE,QAAA;AACA,MAAAC,WAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAN;EAAA;EACAO,KAAA;IACA;MACAC,MAAA;MACAC,OAAA;MACAC,IAAA;MACAC,SAAA;QACAC,EAAA;QACAP,IAAA;QACAQ,IAAA;QACAC,KAAA;QACAC,IAAA;QACAC,GAAA;QACAC,IAAA;QACAC,KAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,GAAApB,QAAA,CAAAI,QAAA;IACA,GAAAJ,QAAA,CAAAK,WAAA;EACA;EACAgB,OAAA;IACAC,aAAA;MACAC,OAAA,CAAAC,GAAA,MAAAf,MAAA;IACA;IACAgB,OAAA;MACAF,OAAA,CAAAC,GAAA;MACA,KAAAE,KAAA;IACA;IACAC,eAAA;MACA,KAAAC,WAAA;MACA,KAAAC,YAAA;IACA;IACAC,iBAAA;MACA,KAAAF,WAAA;MACA,KAAAG,eAAA;IACA;IACAH,YAAA;MACA,KAAAjB,IAAA,SAAAA,IAAA;IACA;IACAkB,aAAA;MACAN,OAAA,CAAAC,GAAA;MACA,KAAAZ,SAAA;QACAC,EAAA,OAAAmB,QAAA,CAAAC,MAAA;QACA3B,IAAA,OAAA4B,KAAA,CAAAC,GAAA;QACArB,IAAA,OAAAoB,KAAA,CAAAE,GAAA;QACArB,KAAA,EAAAsB,MAAA,MAAAH,KAAA,CAAAI,EAAA;QACAtB,IAAA;QACAC,GAAA,OAAAiB,KAAA,CAAAK,EAAA;QACArB,IAAA,EAAAsB,QAAA,MAAAN,KAAA,CAAAO,GAAA;QACAtB,KAAA,EAAAkB,MAAA,MAAAH,KAAA,CAAAQ,CAAA;MACA;MAEA,SAAAR,KAAA,CAAAK,EAAA;QACA3B,SAAA,CAAAI,IAAA;MACA;MAEAO,OAAA,CAAAC,GAAA,iBAAAZ,SAAA;MACAW,OAAA,CAAAC,GAAA,aAAAmB,IAAA,CAAAC,SAAA,CAAAhC,SAAA;MACAT,IAAA,CACA0C,IAAA,gBAAAF,IAAA,CAAAC,SAAA,CAAAhC,SAAA,GACAkC,IAAA,CAAAC,QAAA;QACAxB,OAAA,CAAAC,GAAA,CAAAuB,QAAA;MACA,GACAC,KAAA,CAAAC,KAAA;QACA1B,OAAA,CAAAC,GAAA,CAAAyB,KAAA;MACA;IACA;IACAlB,gBAAA;EACA;AACA"},"metadata":{},"sourceType":"