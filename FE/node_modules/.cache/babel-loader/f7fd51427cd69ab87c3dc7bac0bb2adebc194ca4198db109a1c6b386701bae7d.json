en = true; // 모달을 열기 위해 상태 변경\n    },\n    closeModal() {\n      this.modalOpen = false;\n    },\n    deleteLikeHouse() {\n      this.likeHouse = {\n        id: this.id,\n        name: this.name,\n        dong: this.dong,\n        jibun: this.jibun,\n        type: this.jibun,\n        wal: this.wal,\n        jeon: this.jeon,\n        floor: this.floor,\n      };\n      http\n        .post(`/like/delete`, JSON.stringify(this.likeHouse))\n        .then((response) => {\n          console.log(response);\n          alert(\"삭제되었습니다!\");\n          this.closeModal();\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AA0CA,SAAAA,WAAA;AACA,MAAAC,IAAA,GAAAD,WAAA;AAEA;EACAE,IAAA;EACAC,UAAA;EACAC,KAAA;IACAC,EAAA,EAAAC,MAAA;IACAC,GAAA,EAAAC,MAAA;IACAC,QAAA,EAAAH,MAAA;IACAJ,IAAA,EAAAI,MAAA;IACAI,IAAA,EAAAJ,MAAA;IACAK,KAAA,EAAAL,MAAA;IACAM,IAAA,EAAAN,MAAA;IACAO,GAAA,EAAAP,MAAA;IACAQ,IAAA,EAAAR,MAAA;IACAS,KAAA,EAAAT,MAAA;IACAU,GAAA,EAAAR;EACA;EACAS,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,SAAA;QACAd,EAAA;QACAH,IAAA;QACAQ,IAAA;QACAC,KAAA;QACAC,IAAA;QACAC,GAAA;QACAC,IAAA;QACAC,KAAA;QACAK,OAAA;QACAJ,GAAA;MACA;IACA;EACA;EAEAK,QAAA;IACA,KAAAD,OAAA,GAAAE,IAAA,CAAAP,KAAA,CAAAO,IAAA,CAAAC,MAAA;EACA;EAEAC,OAAA;IACAC,UAAA;MACA,KAAAP,SAAA;IACA;;IACAQ,WAAA;MACA,KAAAR,SAAA;IACA;IACAS,gBAAA;MACA,KAAAR,SAAA;QACAd,EAAA,OAAAA,EAAA;QACAH,IAAA,OAAAA,IAAA;QACAQ,IAAA,OAAAA,IAAA;QACAC,KAAA,OAAAA,KAAA;QACAC,IAAA,OAAAD,KAAA;QACAE,GAAA,OAAAA,GAAA;QACAC,IAAA,OAAAA,IAAA;QACAC,KAAA,OAAAA;MACA;MACAd,IAAA,CACA2B,IAAA,iBAAAC,IAAA,CAAAC,SAAA,MAAAX,SAAA,GACAY,IAAA,CAAAC,QAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,QAAA;QACAG,KAAA;QACA,KAAAT,UAAA;MACA,GACAU,KAAA,CAAAC,KAAA;QACAJ,OAAA,CAAAC,GAAA,CAAAG,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}