     console.log(\"선택된 주소\", addr);\n      this.$emit(\"addr\", addr);\n      this.$emit(\"flag\", true);\n    },\n    moveCenterOnly(index) {\n      // 지역 코드 찾는 for문\n      for (let i = 0; i < 18; i++) {\n        if (String(this.offi[index].지역코드).substr(0, 2) == this.sidos[i].value) {\n          this.sidoname = this.sidos[i].text;\n          break;\n        }\n      }\n      let addr = this.sidoname + \" \" + this.offi[index].법정동 + \" \" + this.offi[index].지번;\n      console.log(\"선택된 주소\", addr);\n      this.$emit(\"addr\", addr);\n      this.$emit(\"flag\", false);\n    },\n    isCheck() {\n      console.log(this.detail);\n      http\n        .post(`/like/getOne`, this.detail)\n        .then((response) => {\n          console.log(response);\n          console.log(\"getOne true\");\n          this.check = true;\n        })\n        .catch((error) => {\n          console.log(error);\n          console.log(\"getOne false\");\n          this.check = false;\n        });\n    },\n    clickDetail(o, index) {\n      this.moveCenterOnly(index);\n      this.goDetail(o, (index + 1) % 20);\n      this.isCheck();\n    },\n    setCheck(tf) {\n      this.check = tf;\n    },\n  },\n  watch: {\n    officetels() {\n      this.offi = this.officetels;\n      // console.log(\"offi\", this.offi);\n    },\n    house() {\n      this.detail = this.house;\n    },\n  },\n};\n</script>\n<style scoped></style>\n"],"mappings":"AAgCA,SAAAA,QAAA,EAAAC,UAAA,EAAAC,YAAA;AACA,SAAAC,WAAA;AAEA,MAAAC,IAAA,GAAAD,WAAA;AACA,MAAAE,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,MAAA;QACAC,EAAA;QACAC,GAAA;QACAC,EAAA;QACAC,GAAA;QACAC,GAAA;QACAC,EAAA;QACAC,CAAA;QACAC,IAAA;QACAC,GAAA;MACA;MACAC,KAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,GAAArB,QAAA,CAAAK,QAAA;EACA;EACAiB,OAAA;IACA,GAAArB,UAAA,CAAAI,QAAA;IACA,GAAAH,YAAA,CAAAG,QAAA;IACAkB,SAAAC,CAAA,EAAAC,MAAA;MACA,KAAAC,KAAA;MACA,KAAAjB,MAAA,CAAAE,GAAA,GAAAa,CAAA,CAAAb,GAAA;MACA,KAAAF,MAAA,CAAAG,EAAA,GAAAY,CAAA,CAAAZ,EAAA;MACA,KAAAH,MAAA,CAAAI,GAAA,GAAAW,