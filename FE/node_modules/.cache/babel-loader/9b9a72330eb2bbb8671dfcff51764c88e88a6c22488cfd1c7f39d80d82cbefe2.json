{"ast":null,"code":"import { mapState, mapActions, mapMutations } from \"vuex\";\nconst mapStore = \"mapStore\";\nexport default {\n  name: \"OfficeList\",\n  data() {\n    return {\n      offi: [],\n      detail: {\n        보증금: 0,\n        월세: 0,\n        건물명: \"\",\n        법정동: \"\",\n        지번: \"\",\n        층: \"\",\n        전용면적: \"\",\n        이미지: \"\"\n      },\n      cards: [] // 카드의 hover 상태를 저장할 배열\n    };\n  },\n\n  computed: {\n    ...mapState(mapStore, [\"sidos\", \"guguns\", \"dongs\", \"officetels\"])\n  },\n  methods: {\n    ...mapActions(mapStore, [\"setHouseDetail\"]),\n    ...mapMutations(mapStore, [\"SET_DETAIL_HOUSE\"]),\n    goDetail(o, imgnum) {\n      this.$emit(\"changeToggle1\", false);\n      this.detail.보증금 = o.보증금;\n      this.detail.월세 = o.월세;\n      this.detail.건물명 = o.단지;\n      this.detail.법정동 = o.법정동;\n      this.detail.지번 = o.지번;\n      this.detail.층 = o.층;\n      this.detail.전용면적 = o.전용면적;\n      if (isNaN(imgnum)) this.detail.이미지 = 0;else this.detail.이미지 = imgnum;\n      this.setHouseDetail(this.detail);\n    }\n  },\n  watch: {\n    officetels() {\n      this.offi = this.officetels;\n      console.log(\"offi\", this.offi);\n    },\n    house() {\n      this.detail = this.house;\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapActions","mapMutations","mapStore","name","data","offi","detail","보증금","월세","건물명","법정동","지번","층","전용면적","이미지","cards","computed","methods","goDetail","o","imgnum","$emit","단지","isNaN","setHouseDetail","watch","officetels","console","log","house"],"sources":["src/components/omap/OfficeList.vue"],"sourcesContent":["<template>\n  <div>\n    <b-card\n      :img-src=\"require('@/assets/' + ((index + 1) % 20) + '.jpg')\"\n      img-left\n      class=\"mb-3\"\n      v-model=\"offi\"\n      v-for=\"(o, index) in offi\"\n      :key=\"index\"\n      :class=\"{ 'hover-effect': cards[index].hover }\"\n      @mouseenter=\"cards[index].hover = true\"\n      @mouseleave=\"cards[index].hover = false\"\n    