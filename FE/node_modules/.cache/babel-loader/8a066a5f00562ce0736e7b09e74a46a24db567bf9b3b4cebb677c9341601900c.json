{"ast":null,"code":"import { mapState, mapActions, mapMutations } from \"vuex\";\nimport HouseMenuSideBar from \"./HouseMenuSideBar.vue\";\nimport { apiInstance } from \"@/api/index.js\";\nimport { writeRecent } from \"@/api/recent.js\";\nconst http = apiInstance();\nconst mapStore = \"mapStore\";\nconst memberStore = \"memberStore\";\nexport default {\n  name: \"HouseDetail\",\n  components: {\n    HouseMenuSideBar\n  },\n  props: {\n    toggle: Number\n  },\n  data() {\n    return {\n      around: \"\",\n      arounds: [\"스터디카페\", \"도서관\", \"지하철역\"]\n    };\n  },\n  created() {\n    writeRecent(this.house, response => {\n      console.log(response);\n      console.log(\"최근목록 실행\");\n    }, error => {\n      console.log(error);\n    });\n  },\n  computed: {\n    ...mapState(mapStore, [\"house\", \"keyword\", \"house\", \"selectsido\", \"sidos\", \"houseLatLng\"]),\n    ...mapState(memberStore, [\"userInfo\"]),\n    pyeong() {\n      return this.house.area * 0.3025;\n    }\n  },\n  methods: {\n    ...mapActions(mapStore, [\"setKeyword\", \"setUnivList\"]),\n    ...mapMutations(mapStore, [\"SET_KEYWORD\"]),\n    searchAround() {\n      console.log(this.around);\n    },\n    goList() {\n      console.log(\"goList?\");\n      this.$emit(\"changeToggle3\", true);\n    },\n    changeBothFunc() {\n      this.checkLikeHouse();\n    },\n    unchangeBothFunc() {\n      this.deleteLikeHouse();\n    },\n    changeKeyword(keyword) {\n      //var addr = this.house.dong +\" \"+ +\" \" +keyword;\n      var addr = this.house.dong + \" \" + this.house.jibun + \" \" + keyword;\n      this.setKeyword(addr);\n    },\n    getUnivList() {\n      let latlng = {\n        lat: this.houseLatLng.lat,\n        lng: this.houseLatLng.lng\n      };\n      this.setUnivList(latlng);\n    },\n    checkLikeHouse() {\n      http.post(`/like/getOne`, this.house).then(response => {\n        console.log(\"getOne resp\", response);\n        this.putLikeHouse();\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    putLikeHouse() {\n      console.log(\"찜목록에 추가된 하우스\", this.house);\n      console.log(\"stringfy\", JSON.stringify(this.house));\n      http.post(`/like/write`, JSON.stringify(this.house)).then(response => {\n        this.$emit(\"toggle\", 0);\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    deleteLikeHouse() {\n      console.log(\"찜목록에 삭제된 하우스\", this.house);\n      http.post(`/like/delete`, JSON.stringify(this.house)).then(response => {\n        this.$emit(\"toggle\", 1);\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    aa() {\n      console.log(\"toggle??????\", this.toggle);\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapActions","mapMutations","HouseMenuSideBar","apiInstance","writeRecent","http","mapStore","memberStore","name","components","props","toggle","Number","data","around","arounds","created","house","response","console","log","error","computed","pyeong","area","methods","searchAround","goList","$emit","changeBothFunc","checkLikeHouse","unchangeBothFunc","deleteLikeHouse","changeKeyword","keyword","addr","dong","jibun","setKeyword","getUnivList","latlng","lat","houseLatLng","lng","setUnivList","post","then","putLikeHouse","catch","JSON","stringify","aa"],"sources":["src/components/map/HouseDetail.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- <b-button @click=\"aa\" type=\"button\">{{ this.toggle }}</b-button> -->\n    <hr />\n    <b-container style=\"position: relative\">\n      <div style=\"position: relative; z-index: 1\">\n        <b-img :src=\"require('@/assets/' + house.img + '.jpg')\" style=\"width: 80%\"></b-img>\n        <div style=\"position: absolute; top: 30px; right: 100px\">\n          <b-icon\n            icon=\"heart\"\n            scale=\"3\"\n            @click=\"changeBothFunc\"\n            v-if=\"toggle == 1\"\n            style=\"color: red\"\n          ></b-icon>\n          <b-icon\n            icon=\"heart-fill\"\n            scale=\"3\"\n            @click=\"unchangeBothFunc\"\n            v-else\n            style=\"color: red\"\n          ></b-icon>\n        </div>\n      </div>\n      <hr />\n      <b-list-group>\n        <b-list-group-item v-if=\"house.wal == 0\">\n          <b-row>\n            <b-col>\n              <b-img :src=\"require('@/assets/price.png')\" style=\"width: 20%; float: left\"></b-img>\n            </b-col>\n            <b-col>\n              <div clss=\"content\" style=\"float: left; margin-left: 5%; margin-right: 10%\">\n                <strong>전세</strong>\n              </div>\n            </b-col>\n            <b-col>\n              <div style=\"float: left\">{{ house.jeon }} 만원</div>\n            </b-col>\n          </b-row>\n        </b-list-group-item>\n\n        <b-row v-else>\n          <b-list-group-item>\n            <b-col>\n              <b-img :src=\"require('@/assets/price.png')\" style=\"width: 6%; float: left\"></b-img>\n            </b-col>\n            <b-col>\n              <div clss=\"content\" style=\"float: left; margin-left: 5%; margin-right: 10%\">\n                <strong>월세</strong>\n              </div>\n            </b-col>\n            <b-col>\n              <div style=\"float: left\">{{ house.jeon }} 만원 / {{ house.wal }} 만원</div>\n            </b-col>\n          </b-list-group-item>\n        </b-row>\n\n        <b-list-group-item>\n          <b-img :src=\"require('@/assets/title.png')\" style=\"width: 6%; float: left\"></b-img>\n          <div clss=\"content\" style=\"float: left; margin-left: 5%; margin-right: 10%\">\n            <strong>건물명</strong>\n          </div>\n          <div style=\"float: left\">{{ house.name }}</div>\n        </b-list-group-item>\n\n        <b-list-group-item>\n          <b-img :src=\"require('@/assets/address.png')\" style=\"width: 6%; float: left\"></b-img>\n          <div clss=\"content\" style=\"float: left; margin-left: 5%; margin-right: 10%\">\n            <strong>주소</strong>\n          </div>\n          <div style=\"float: left\">{{ house.dong }}-{{ house.jibun }} {{ house.floor }}층</div>\n        </b-list-group-item>\n\n        <b-list-group-item>\n          <b-img :src=\"require('@/assets/area.png')\" style=\"width: 6%; float: left\"></b-img>\n          <div clss=\"content\" style=\"float: left; margin-left: 5%; margin-right: 10%\">\n            <strong>전용면적</strong>\n          </div>\n          <div style=\"float: left\">{{ house.area }}㎡ ( {{ pyeong.toFixed(2) }} 평)</div>\n        </b-list-group-item>\n\n        <b-list-group-item>\n          <b-img :src=\"require('@/assets/calender.png')\" style=\"width: 6%; float: left\"></b-img>\n          <div clss=\"content\" style=\"float: left; margin-left: 5%; margin-right: 10%\">\n            <strong>건축연도</strong>\n          </div>\n          <div style=\"float: left\">{{ house.construction }}년</div>\n        </b-list-group-item>\n      </b-list-group>\n    </b-container>\n    <hr />\n    <b-container>\n      <b-row>\n        <b-col>\n          <!-- :img-src=\"require('@/assets/libraryImg.png')\" -->\n          <b-button pill variant=\"outline-light\" @click=\"changeKeyword('도서관')\"\n            ><b-img :src=\"require('@/assets/libraryImg.png')\" style=\"width: 110%\"></b-img\n          ></b-button>\n        </b-col>\n        <b-col>\n          <b-button pill variant=\"outline-light\" @click=\"changeKeyword('스터디 카페')\"\n            ><b-img :src=\"require('@/assets/studyImg.png')\" style=\"width: 110%\"></b-img\n          ></b-button>\n        </b-col>\n        <b-col>\n          <b-button pill variant=\"outline-light\" @click=\"changeKeyword('편의점')\"\n            ><b-img :src=\"require('@/assets/storeImg.png')\" style=\"width: 110%\"></b-img\n          ></b-button>\n        </b-col>\n        <b-col>\n          <b-button pill variant=\"outline-light\" @click=\"changeKeyword('지하철역')\"\n            ><b-img :src=\"require('@/assets/subwayImg.png')\" style=\"width: 110%\"></b-img\n          ></b-button>\n        </b-col>\n      </b-row>\n    </b-container>\n\n    <b-container class=\"mb-3\">\n      <b-button size=\"lg\" style=\"float: right; width: 100%; margin-top: 10px\" @click=\"getUnivList\"\n        >주변 대학 검색</b-button\n      >\n    </b-container>\n    <b-container class=\"mb-3\">\n      <b-button v-b-toggle.sidebar-1 size=\"lg\" style=\"float: right; width: 100%; margin-top: 10px\"\n        >지원 제도 검색</b-button\n      >\n    </b-container>\n\n    <house-menu-side-bar></house-menu-side-bar>\n    <router-view />\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from \"vuex\";\nimport HouseMenuSideBar from \"./HouseMenuSideBar.vue\";\nimport { apiInstance } from \"@/api/index.js\";\nimport { writeRecent } from \"@/api/recent.js\";\n\nconst http = apiInstance();\nconst mapStore = \"mapStore\";\nconst memberStore = \"memberStore\";\n\nexport default {\n  name: \"HouseDetail\",\n  components: { HouseMenuSideBar },\n  props: {\n    toggle: Number,\n  },\n  data() {\n    return {\n      around: \"\",\n      arounds: [\"스터디카페\", \"도서관\", \"지하철역\"],\n    };\n  },\n  created() {\n    writeRecent(\n      this.house,\n      (response) => {\n        console.log(response);\n        console.log(\"최근목록 실행\");\n      },\n      (error) => {\n        console.log(error);\n      }\n    );\n  },\n  computed: {\n    ...mapState(mapStore, [\"house\", \"keyword\", \"house\", \"selectsido\", \"sidos\", \"houseLatLng\"]),\n    ...mapState(memberStore, [\"userInfo\"]),\n    pyeong() {\n      return this.house.area * 0.3025;\n    },\n  },\n  methods: {\n    ...mapActions(mapStore, [\"setKeyword\", \"setUnivList\"]),\n    ...mapMutations(mapStore, [\"SET_KEYWORD\"]),\n    searchAround() {\n      console.log(this.around);\n    },\n    goList() {\n      console.log(\"goList?\");\n      this.$emit(\"changeToggle3\", true);\n    },\n    changeBothFunc() {\n      this.checkLikeHouse();\n    },\n    unchangeBothFunc() {\n      this.deleteLikeHouse();\n    },\n    changeKeyword(keyword) {\n      //var addr = this.house.dong +\" \"+ +\" \" +keyword;\n      var addr = this.house.dong + \" \" + this.house.jibun + \" \" + keyword;\n      this.setKeyword(addr);\n    },\n    getUnivList() {\n      let latlng = { lat: this.houseLatLng.lat, lng: this.houseLatLng.lng };\n      this.setUnivList(latlng);\n    },\n    checkLikeHouse() {\n      http\n        .post(`/like/getOne`, this.house)\n        .then((response) => {\n          console.log(\"getOne resp\", response);\n          this.putLikeHouse();\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    putLikeHouse() {\n      console.log(\"찜목록에 추가된 하우스\", this.house);\n      console.log(\"stringfy\", JSON.stringify(this.house));\n      http\n        .post(`/like/write`, JSON.stringify(this.house))\n        .then((response) => {\n          this.$emit(\"toggle\", 0);\n          console.log(response);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    deleteLikeHouse() {\n      console.log(\"찜목록에 삭제된 하우스\", this.house);\n      http\n        .post(`/like/delete`, JSON.stringify(this.house))\n        .then((response) => {\n          this.$emit(\"toggle\", 1);\n          console.log(response);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    aa() {\n      console.log(\"toggle??????\", this.toggle);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.mb-3 {\n  margin-top: 20px;\n}\n.content {\n  width: 500px;\n  margin-left: 50px;\n}\n</style>\n"],"mappings":"AAuIA,SAAAA,QAAA,EAAAC,UAAA,EAAAC,YAAA;AACA,OAAAC,gBAAA;AACA,SAAAC,WAAA;AACA,SAAAC,WAAA;AAEA,MAAAC,IAAA,GAAAF,WAAA;AACA,MAAAG,QAAA;AACA,MAAAC,WAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAP;EAAA;EACAQ,KAAA;IACAC,MAAA,EAAAC;EACA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACAZ,WAAA,CACA,KAAAa,KAAA,EACAC,QAAA;MACAC,OAAA,CAAAC,GAAA,CAA