{"ast":null,"code":"import HouseList from \"@/components/map/HouseList.vue\";\nimport HouseDetail from \"@/components/map/HouseDetail.vue\";\nimport HouseSearchBar from \"@/components/map/HouseSearchBar.vue\";\nexport default {\n  name: \"HouseListParent\",\n  components: {\n    HouseList,\n    HouseDetail,\n    HouseSearchBar\n  },\n  data() {\n    return {\n      c: true,\n      toggle: 0\n    };\n  },\n  created() {},\n  methods: {\n    changeView1(value) {\n      this.c = value;\n    },\n    changeView2(value) {\n      this.c = value;\n    },\n    moveCenter(addr) {\n      this.$emit(\"addr\", addr);\n      // console.log(\"list 부모 addr\", addr);\n    },\n\n    moveCenterOnly(flag) {\n      this.$emit(\"flag\", flag);\n      // console.log(\"list 부모 addr\", addr);\n    },\n\n    setCheck(value) {\n      this.toggle = value;\n      console.log(\"list에서 전달된 check\", this.toggle);\n    },\n    setToggle(value) {\n      this.toggle = value;\n      console.log(\"detail에서 전달된 toggle\", this.toggle);\n    }\n  }\n};","map":{"version":3,"names":["HouseList","HouseDetail","HouseSearchBar","name","components","data","c","toggle","created","methods","changeView1","value","changeView2","moveCenter","addr","$emit","moveCenterOnly","flag","setCheck","console","log","setToggle"],"sources":["src/components/map/HouseListParent.vue"],"sourcesContent":["<template>\n  <div id=\"houseListParent\">\n    <div id=\"searchBar\">\n      <house-search-bar @changeToggle2=\"changeView2\"></house-search-bar>\n    </div>\n    <div id=\"houseDiv\">\n      <template v-if=\"c\">\n        <house-list\n          @changeToggle1=\"changeView1\"\n          @addr=\"moveCenter\"\n          @flag=\"moveCenterOnly\"\n          @check=\"setCheck\"\n        ></house-list>\n      </template>\n      <template v-else>\n        <house-detail\n          @toggle=\"setToggle\"\n          :toggle=\"toggle\"\n          @changeToggle3=\"changeView2\"\n        ></house-detail>\n      </template>\n    </div>\n    <router-view />\n  </div>\n</template>\n\n<script>\nimport HouseList from \"@/components/map/HouseList.vue\";\nimport HouseDetail from \"@/components/map/HouseDetail.vue\";\nimport HouseSearchBar from \"@/components/map/HouseSearchBar.vue\";\n\nexport default {\n  name: \"HouseListParent\",\n  components: { HouseList, HouseDetail, HouseSearchBar },\n  data() {\n    return {\n      c: true,\n      toggle: 0,\n    };\n  },\n  created() {},\n  methods: {\n    changeView1(value) {\n      this.c = value;\n    },\n    changeView2(value) {\n      this.c = value;\n    },\n    moveCenter(addr) {\n      this.$emit(\"addr\", addr);\n      // console.log(\"list 부모 addr\", addr);\n    },\n    moveCenterOnly(flag) {\n      this.$emit(\"flag\", flag);\n      // console.log(\"list 부모 addr\", addr);\n    },\n    setCheck(value) {\n      this.toggle = value;\n      console.log(\"list에서 전달된 check\", this.toggle);\n    },\n    setToggle(value) {\n      this.toggle = value;\n      console.log(\"detail에서 전달된 toggle\", this.toggle);\n    },\n  },\n};\n</script>\n\n<style scoped>\n#houseListParent {\n  text-align: center;\n}\n#searchBar {\n  display: inline-block;\n}\n#houseDiv {\n  overflow: auto;\n  height: 900px;\n}\ntemplate {\n  margin: 0;\n}\ndiv {\n  margin: 0;\n}\n.container {\n  /* // width: 100vw; */\n  height: 80vh;\n}\n</style>\n"],"mappings":"AA2BA,OAAAA,SAAA;AACA,OAAAC,WAAA;AACA,OAAAC,cAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAJ,SAAA;IAAAC,WAAA;IAAAC;EAAA;EACAG,KAAA;IACA;MACAC,CAAA;MACAC,MAAA;IACA;EACA;EACAC,QAAA;EACAC,OAAA;IACAC,YAAAC,KAAA;MACA,KAAAL,CAAA,GAAAK,KAAA;IACA;IACAC,YAAAD,KAAA;MACA,KAAAL,CAAA,GAAAK,KAAA;IACA;IACAE,WAAAC,IAAA;MACA,KAAAC,KAAA,SAAAD,IAAA;MACA;IACA;;IACAE,eAAAC,IAAA;MACA,KAAAF,KAAA,SAAAE,IAAA;MACA;IACA;;IACAC,SAAAP,KAAA;MACA,KAAAJ,MAAA,GAAAI,KAAA;MACAQ,OAAA,CAAAC,GAAA,0BAAAb,MAAA;IACA;IACAc,UAAAV,KAAA;MACA,KAAAJ,MAAA,GAAAI,KAAA;MACAQ,OAAA,CAAAC,GAAA,6BAAAb,MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}