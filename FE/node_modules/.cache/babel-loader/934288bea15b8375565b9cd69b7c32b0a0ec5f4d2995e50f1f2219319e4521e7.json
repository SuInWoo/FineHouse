{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { listArticle } from \"@/api/board.js\";\nimport BoardListItem from \"@/components/board/BoardListItem\";\nimport { mapState } from \"vuex\";\nconst memberStore = \"memberStore\";\nexport default {\n  name: \"BoardList\",\n  components: {\n    BoardListItem\n  },\n  data() {\n    return {\n      articles: []\n    };\n  },\n  computed: {\n    ...mapState(memberStore, [\"userInfo\"])\n  },\n  created() {\n    let param = {\n      pg: 1,\n      spp: 20,\n      key: null,\n      word: null\n    };\n    listArticle(param, response => {\n      this.articles = response.data;\n    }, error => {\n      console.log(error);\n    });\n  },\n  methods: {\n    moveWrite() {\n      console.log(this.userInfo);\n      if (this.$route.path !== \"/board/write\") this.$router.push(\"/board/write\");\n    }\n  }\n};","map":{"version":3,"names":["listArticle","BoardListItem","mapState","memberStore","name","components","data","articles","computed","created","param","pg","spp","key","word","response","error","console","log","methods","moveWrite","userInfo","$route","path","$router","push"],"sources":["src/components/board/BoardList.vue"],"sourcesContent":["<template>\n  <b-container class=\"bv-example-row mt-3\">\n    <b-row>\n      <b-col>\n        <h4>공지사항</h4>\n      </b-col>\n    </b-row>\n    <b-row class=\"mb-1\">\n      <b-col class=\"text-right\">\n        <!-- v-if=\"userInfo.writer === 'admin'\" -->\n        <!-- <b-button variant=\"outline-primary\">{{ userInfo.userId }}</b-button> -->\n        <b-button v-if=\"userInfo.admin == true\" variant=\"outline-dark\" @click=\"moveWrite\"\n          >글쓰기</b-button\n        >\n      </b-col>\n    </b-row>\n    <b-row>\n      <b-col v-if=\"articles.length\">\n        <b-table-simple hover responsive>\n          <b-thead head-variant=\"info\">\n            <b-tr>\n              <b-th>글번호</b-th>\n              <b-th>제목</b-th>\n              <b-th>작성자</b-th>\n              <b-th>작성일</b-th>\n            </b-tr>\n          </b-thead>\n\n          <tbody>\n            <!-- 하위 component인 ListRow에 데이터 전달(props) -->\n            <board-list-item v-for=\"article in articles\" :key=\"article.bno\" v-bind=\"article\" />\n          </tbody>\n        </b-table-simple>\n      </b-col>\n\n      <!-- <b-col v-else class=\"text-center\">도서 목록이 없습니다.</b-col> -->\n    </b-row>\n  </b-container>\n</template>\n\n<script>\nimport { listArticle } from \"@/api/board.js\";\nimport BoardListItem from \"@/components/board/BoardListItem\";\nimport { mapState } from \"vuex\";\n\nconst memberStore = \"memberStore\";\n\nexport default {\n  name: \"BoardList\",\n  components: {\n    BoardListItem,\n  },\n  data() {\n    return {\n      articles: [],\n    };\n  },\n  computed: {\n    ...mapState(memberStore, [\"userInfo\"]),\n  },\n  created() {\n    let param = {\n      pg: 1,\n      spp: 20,\n      key: null,\n      word: null,\n    };\n    listArticle(\n      param,\n      (response) => {\n        this.articles = response.data;\n      },\n      (error) => {\n        console.log(error);\n      }\n    );\n  },\n  methods: {\n    moveWrite() {\n      console.log(this.userInfo);\n      if (this.$route.path !== \"/board/write\") this.$router.push(\"/board/write\");\n    },\n  },\n};\n</script>\n\n<style scope>\n.tdClass {\n  width: 50px;\n  text-align: center;\n}\n.tdSubject {\n  width: 300px;\n  text-align: left;\n}\n</style>\n"],"mappings":";AAyCA,SAAAA,WAAA;AACA,OAAAC,aAAA;AACA,SAAAC,QAAA;AAEA,MAAAC,WAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAJ;EACA;EACAK,KAAA;IACA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAN,QAAA,CAAAC,WAAA;EACA;EACAM,QAAA;IACA,IAAAC,KAAA;MACAC,EAAA;MACAC,GAAA;MACAC,GAAA;MACAC,IAAA;IACA;IACAd,WAAA,CACAU,KAAA,EACAK,QAAA;MACA,KAAAR,QAAA,GAAAQ,QAAA,CAAAT,IAAA;IACA,GACAU,KAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;IACA,CACA;EACA;EACAG,OAAA;IACAC,UAAA;MACAH,OAAA,CAAAC,GAAA,MAAAG,QAAA;MACA,SAAAC,MAAA,CAAAC,IAAA,0BAAAC,OAAA,CAAAC,IAAA;IACA;EACA;AACA"},"metadata":{